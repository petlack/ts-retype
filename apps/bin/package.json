{
    "name": "@ts-retype/bin",
    "sideEffects": false,
    "version": "0.3.0-dev.16",
    "description": "",
    "type": "module",
    "main": "dist/index.cjs",
    "bin": {
        "ts-retype": "dist/ts-retype.cjs"
    },
    "scripts": {
        "bin": "./dist/release/ts-retype.cjs .",
        "build": "tsup",
        "build:watch": "tsup --watch --onSuccess \"npm run dev\"",
        "compile": "cp -R ./dist/release/* ./dist/",
        "clean": "rm -rf ./dist",
        "dev": "node dist/index.cjs",
        "publish": "cd ./dist/release && npm publish",
        "release": "pnpm --filter @ts-retype/dev run-bump -x --level dev",
        "start": "node dist/index.cjs --help",
        "stats": "rollup -c rollup.config.mjs",
        "test": "vitest run --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@ts-retype/search": "workspace:^",
        "@ts-retype/utils": "workspace:^",
        "commander": "^12.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@types/node": "^20.11.25",
        "@vitest/coverage-v8": "^1.3.1",
        "tsup": "^8.0.2",
        "vitest": "^1.3.1"
    },
    "nx": {
        "targets": {
            "release": {
                "dependsOn": [
                    "@ts-retype/bin:compile",
                    "@ts-retype/dev:build"
                ]
            },
            "bin": {
                "dependsOn": [
                    "compile"
                ]
            },
            "publish": {
                "dependsOn": [
                    "compile"
                ]
            },
            "compile": {
                "dependsOn": [
                    "@ts-retype/dev:run-bin",
                    "build"
                ]
            },
            "build": {
                "dependsOn": [
                    "@ts-retype/vis:build"
                ]
            }
        }
    }
}
