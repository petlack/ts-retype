{
    "targetDefaults": {
        "build": {
            "cache": true,
            "dependsOn": [
                "lint",
                "^build"
            ],
            "outputs": [
                "{projectRoot}/dist"
            ],
            "inputs": [
                "production",
                "^production"
            ]
        },
        "test": {
            "cache": true,
            "dependsOn": [
                "build",
                "^build"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ],
            "cache": true
        }
    },
    "affected": {
        "defaultBase": "main"
    },
    "namedInputs": {
        "default": [
            "{projectRoot}/**/*",
            "sharedGlobals"
        ],
        "sharedGlobals": [],
        "production": [
            "default"
        ]
    }
}
