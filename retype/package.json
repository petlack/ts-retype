{
  "name": "@ts-retype/retype",
  "version": "0.2.6-rc",
  "publishConfig": {
    "access": "public"
  },
  "description": "Discover duplicate TypeScript types in your codebase.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petlack/ts-retype"
  },
  "author": "petlack",
  "homepage": "https://petlack.github.io/ts-retype/",
  "bugs": "https://github.com/petlack/ts-retype/issues",
  "main": "dist/index.js",
  "bin": {
    "ts-retype": "dist/ts-retype.cjs"
  },
  "type": "module",
  "scripts": {
    "bin": "./dist/ts-retype.cjs",
    "bin:dev": "rollup -c && ./dist/ts-retype.cjs",
    "bin:perf": "npm run build && node --stack-size=2000 --max-old-space-size=32768 dist/ts-retype.cjs",
    "bin:rebuild": "npm run build && ./dist/ts-retype.cjs",
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "dev": "nodemon -I --exec node --",
    "prebuild": "npm run clean",
    "publish": "cd dist && npm publish",
    "start": "node --",
    "transpile": "tsc",
    "watch": "rollup -w -c rollup.config.dev.js"
  },
  "files": [
    "dist/index.js",
    "dist/types.js"
  ],
  "keywords": [
    "typescript",
    "static-analysis",
    "nodejs",
    "cli"
  ],
  "dependencies": {
    "commander": "^10.0.0",
    "date-fns": "^2.29.3",
    "glob": "^9.1.0",
    "progress": "^2.0.3",
    "ramda": "^0.28.0",
    "refractor": "^4.8.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.4.0",
    "@types/progress": "^2.0.5",
    "@types/ramda": "^0.28.23",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-rollup": "^3.0.0",
    "jest": "^29.4.3",
    "lint-staged": "^11.0.1",
    "magic-string": "^0.30.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.1",
    "rollup": "^3.17.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
